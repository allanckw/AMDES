    <!-- controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" for tab control error -->
    <pages maintainScrollPositionOnPostBack="true" controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
