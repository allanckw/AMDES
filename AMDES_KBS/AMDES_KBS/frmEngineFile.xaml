<Window x:Class="AMDES_KBS.frmEngineFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="frmQuestionGroupSetting" Height="110" Width="400" WindowStyle="SingleBorderWindow" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,10,0,0">
                <Label x:Name="lblSym" Width="80" Content="File Name:" FontWeight="Bold" FontSize="14" Margin="5,-5,0,0" />
                <TextBox x:Name="txtFileName" Width="280" BorderThickness="1" Background="Transparent"  Margin="0,0,0,0" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button x:Name="btnOK" Content="OK" Width="80" Margin="10" FontSize="14" Click="btnOK_Click"></Button>
                <Button x:Name="btnCancel" Content="Cancel" Width="80" Margin="10" FontSize="14" Click="btnCancel_Click"></Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
